{
	"compilerOptions": {
		"declaration": true,
		"declarationMap": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"importHelpers": false,
		"lib": ["es2020", "es5"],
		"module": "es2022",
		"moduleResolution": "node16",
		"noEmitOnError": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"noUnusedLocals": true,
		"preserveSymlinks": true,
		"preserveWatchOutput": true,
		"pretty": false,
		"rootDir": "./src",
		"strict": true,
		"sourceMap": true,
		"target": "es2022"
	},
	"exclude": [],
	"include": ["./src/**/*.ts"]
}
